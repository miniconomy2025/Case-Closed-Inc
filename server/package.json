    "@aws-sdk/client-sqs": "^3.844.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^17.0.0",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "install": "^0.13.0",
    "joi": "^17.13.3",
    "knex": "^3.1.0",
    "node-cron": "^4.1.1",
    "npm": "^11.4.2",
    "pg": "^8.16.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
